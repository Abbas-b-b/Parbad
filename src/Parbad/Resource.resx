  <data name="LogViewerHtml" xml:space="preserve">
    <value>&lt;!DOCTYPE html&gt;

&lt;html lang="en" xmlns="http://www.w3.org/1999/xhtml"&gt;
&lt;head&gt;
    &lt;title&gt;Parbad Logs&lt;/title&gt;
    &lt;meta charset="utf-8"&gt;
    &lt;meta http-equiv="X-UA-Compatible" content="IE=edge"&gt;
    &lt;meta name="viewport" content="width=device-width, initial-scale=1"&gt;
    &lt;link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" /&gt;
    &lt;link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-bootgrid/1.3.1/jquery.bootgrid.min.css" /&gt;
    &lt;style type="text/css"&gt;
        body {
            padding-bottom: 20px;
        }

        .bootgrid-header .search {
            display: none;
        }

        .bootgrid-header .actions &gt; div:last-child {
            display: none;
        }

        .bootgrid-table th &gt; .column-header-anchor {
            color: #333;
            cursor: default;
        }

        .bootgrid-table th:hover {
            color: #333;
            cursor: default;
            background-color: transparent;
        }

        .gridHeaderRow {
            background-color: #0095c1;
            color: #fff;
        }

            .gridHeaderRow span {
                color: #fff;
            }

        .pagination &gt; li &gt; a {
            cursor: pointer;
        }
    &lt;/style&gt;
&lt;/head&gt;
&lt;body&gt;
    &lt;div class="container"&gt;
        &lt;div class="page-header"&gt;
            &lt;h1&gt;Parbad Logs&lt;/h1&gt;
        &lt;/div&gt;
        &lt;div class="table-responsive"&gt;
            &lt;table id="grid" class="table table-hover"&gt;
                &lt;thead&gt;
                    &lt;tr class="gridHeaderRow"&gt;
                        &lt;th data-column-id="type" data-sortable="false" data-searchable="false"&gt;Type&lt;/th&gt;
                        &lt;th data-column-id="gateway" data-sortable="false" data-searchable="false"&gt;Gateway&lt;/th&gt;
                        &lt;th data-column-id="orderNumber" data-converter="digit" data-sortable="false" data-searchable="false"&gt;Order Number&lt;/th&gt;
                        &lt;th data-column-id="amount" data-converter="digit" data-sortable="false" data-searchable="false"&gt;Amount&lt;/th&gt;
                        &lt;th data-column-id="referenceId" data-sortable="false" data-searchable="false"&gt;Reference ID&lt;/th&gt;
                        &lt;th data-column-id="transactionId" data-sortable="false" data-searchable="false"&gt;Transaction ID&lt;/th&gt;
                        &lt;th data-column-id="status" data-sortable="false" data-searchable="false"&gt;Status&lt;/th&gt;
                        &lt;th data-column-id="message" data-sortable="false" data-searchable="false"&gt;Message&lt;/th&gt;
                        &lt;th data-column-id="createdOn" data-formatter="date" data-sortable="false" data-searchable="false"&gt;Date&lt;/th&gt;
                    &lt;/tr&gt;
                &lt;/thead&gt;
            &lt;/table&gt;
        &lt;/div&gt;
    &lt;/div&gt;
    &lt;script type="text/javascript" src="https://code.jquery.com/jquery-3.1.1.min.js"&gt;&lt;/script&gt;
    &lt;script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-bootgrid/1.3.1/jquery.bootgrid.min.js"&gt;&lt;/script&gt;
    &lt;script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js"&gt;&lt;/script&gt;
    &lt;script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"&gt;&lt;/script&gt;
    &lt;script type="text/javascript"&gt;
        function toDigits(number) {
            return parseInt(number, 10).toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,");
        }
        
        var pageNumber = 1;
        var pageSize = 10;

        $("#grid").bootgrid({
            ajax: true,
            url: "#Url#",
            rowCount: [10, 20, 50, 100],
            labels: {
                infos: "Showing {{ctx.start}} to {{ctx.end}} of {{ctx.total}} logs"
            },
            converters: {
                digit: {
                    from: function (value) { return value; },
                    to: function (value) { return toDigits(value); }
                }
            },
            ajaxSettings: {
                method: "GET",
                cache: false
            },
            requestHandler: function (request) {
                pageNumber = request.current;
                pageSize = request.rowCount;

                return {
                    pageNumber: request.current,
                    pageSize: request.rowCount
                };
            },
            responseHandler: function (response) {
                return {
                    current: pageNumber,
                    rowCount: pageSize,
                    total: response.totalLogCount,
                    rows: response.logs
                };
            }
        });
    &lt;/script&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
