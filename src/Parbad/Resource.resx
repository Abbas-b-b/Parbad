  <data name="PaymentPayHtml" xml:space="preserve">
    <value>&lt;!DOCTYPE html&gt;&lt;html&gt;&lt;head&gt; &lt;title&gt;Parbad Gateway&lt;/title&gt; &lt;meta charset="utf-8"&gt; &lt;meta http-equiv="X-UA-Compatible" content="IE=edge"&gt; &lt;meta name="viewport" content="width=device-width, initial-scale=1"&gt; &lt;link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"/&gt; &lt;style type="text/css"&gt; body{margin-top: 30px;}.panel-title{font-weight: bold;}&lt;/style&gt;&lt;/head&gt;&lt;body&gt; &lt;div class="container"&gt; &lt;div class="row"&gt; &lt;div class="col-md-6 col-md-offset-3"&gt; &lt;div class="panel"&gt; &lt;div class="panel-heading"&gt; &lt;h3 class="panel-title text-center"&gt;Parbad Virtual Gateway&lt;/h3&gt; &lt;/div&gt;&lt;div class="panel-body"&gt; &lt;form id="paymentForm" action="#RedirectUrl#" method="post" role="form"&gt; &lt;input name="referenceId" value="#ReferenceId#" type="hidden"/&gt; &lt;input name="transactionId" value="#TransactionId#" type="hidden"/&gt; &lt;input name="result" id="result" value="false" type="hidden"/&gt; &lt;div class="form-group"&gt; &lt;label class="control-label"&gt;Order Number&lt;/label&gt; &lt;p class="form-control-static"&gt;#DisplayOrderNumber#&lt;/p&gt;&lt;/div&gt;&lt;div class="form-group"&gt; &lt;label class="control-label"&gt;Amount&lt;/label&gt; &lt;p class="form-control-static"&gt;#DisplayAmount#&lt;/p&gt;&lt;/div&gt;&lt;div class="form-group"&gt; &lt;label class="control-label"&gt;Reference ID&lt;/label&gt; &lt;p class="form-control-static"&gt;#ReferenceId#&lt;/p&gt;&lt;/div&gt;&lt;div class="form-group"&gt; &lt;label class="control-label"&gt;Transaction ID&lt;/label&gt; &lt;p class="form-control-static"&gt;#TransactionId#&lt;/p&gt;&lt;/div&gt;&lt;div class="form-group"&gt; &lt;label class="control-label"&gt;Status&lt;/label&gt; &lt;p id="status" class="form-control-static"&gt;Payed&lt;/p&gt;&lt;/div&gt;&lt;/form&gt; &lt;/div&gt;&lt;/div&gt;&lt;div class="alert alert-info"&gt; &lt;i class="glyphicon glyphicon-info-sign"&gt;&lt;/i&gt; Page will redirects automatically in &lt;strong id="counter"&gt;5&lt;/strong&gt; second(s)... &lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;script type="text/javascript" src="https://code.jquery.com/jquery-3.1.1.min.js"&gt;&lt;/script&gt; &lt;script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"&gt;&lt;/script&gt; &lt;script type="text/javascript"&gt;var isPayed=#IsPayed#; var timer; var seconds=5; if (isPayed){$(".panel").addClass("panel-success"); $("#status").text("Payed").addClass("text-success"); $("#result").val("true");}else{$(".panel").addClass("panel-danger"); $("#status").text("Not payed").addClass("text-danger"); $("#result").val("false");}function submitForm(){$("#paymentForm").submit();}function countDown(){seconds--; $("#counter").text(seconds); if (seconds &lt;=0){clearInterval(timer); submitForm(); return;}}timer=setInterval(countDown, 1000); &lt;/script&gt;&lt;/body&gt;&lt;/html&gt;</value>
  </data>
  <data name="PaymentRequestHtml" xml:space="preserve">
    <value>&lt;!DOCTYPE html&gt;&lt;html&gt;&lt;head&gt; &lt;title&gt;Parbad Gateway&lt;/title&gt; &lt;meta charset="utf-8"&gt; &lt;meta http-equiv="X-UA-Compatible" content="IE=edge"&gt; &lt;meta name="viewport" content="width=device-width, initial-scale=1"&gt; &lt;link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"/&gt; &lt;style type="text/css"&gt; body{margin-top: 30px;}.panel-title{font-weight: bold;}&lt;/style&gt;&lt;/head&gt;&lt;body&gt; &lt;div class="container"&gt; &lt;div class="row"&gt; &lt;div class="col-md-6 col-md-offset-3"&gt; &lt;div class="panel panel-default"&gt; &lt;div class="panel-heading"&gt; &lt;h3 class="panel-title text-center"&gt;Parbad Virtual Gateway&lt;/h3&gt; &lt;/div&gt;&lt;div class="panel-body"&gt; &lt;form action="#ParbadGatewayUrl#" method="post" role="form"&gt; &lt;input name="commandType" value="Pay" type="hidden"/&gt; &lt;input name="orderNumber" value="#OrderNumber#" type="hidden"/&gt; &lt;input name="amount" value="#Amount#" type="hidden"/&gt; &lt;input name="redirectUrl" value="#RedirectUrl#" type="hidden"/&gt; &lt;input name="isPayed" id="isPayed" value="false" type="hidden"/&gt; &lt;div class="form-group"&gt; &lt;label class="control-label"&gt;Order number&lt;/label&gt; &lt;p class="form-control-static"&gt;#DisplayOrderNumber#&lt;/p&gt;&lt;/div&gt;&lt;div class="form-group"&gt; &lt;label class="control-label"&gt;Amount&lt;/label&gt; &lt;p class="form-control-static"&gt;#DisplayAmount#&lt;/p&gt;&lt;/div&gt;&lt;div class="form-group"&gt; &lt;label class="control-label" for="password"&gt;Password&lt;/label&gt; &lt;div class="input-group"&gt; &lt;input id="password" name="password" type="password" class="form-control"/&gt; &lt;span class="input-group-addon"&gt;&lt;span class="glyphicon glyphicon-lock"&gt;&lt;/span&gt;&lt;/span&gt; &lt;/div&gt;&lt;span class="help-block"&gt; &lt;i class="glyphicon glyphicon-info-sign"&gt;&lt;/i&gt; Enter the password of Parbad Virtual Gateway. &lt;/span&gt; &lt;/div&gt;&lt;button type="submit" class="btn btn-success btn-block" onclick="return pay()"&gt;Pay&lt;/button&gt; &lt;br/&gt; &lt;button type="submit" class="btn btn-danger btn-block" onclick="return cancelPay()"&gt;Cancel&lt;/button&gt; &lt;/form&gt; &lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;script type="text/javascript" src="https://code.jquery.com/jquery-3.1.1.min.js"&gt;&lt;/script&gt; &lt;script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"&gt;&lt;/script&gt; &lt;script type="text/javascript"&gt;var isPasswordRequired=#IsPasswordRequired#; var passwordInput=$("#password"); var passwordFormGroup=passwordInput.parents(".form-group"); if (!isPasswordRequired){passwordFormGroup.remove();}function isNullOrWhiteSpace(value){return value.replace(/\s/g, "").length &lt; 1;}function pay(){if (!isPasswordRequired || (isPasswordRequired &amp;&amp; !isNullOrWhiteSpace(passwordInput.val()))){$("#isPayed").val("true"); return true;}if (!passwordFormGroup.hasClass("has-error")){passwordFormGroup.addClass("has-error");}passwordInput.focus(); return false;}function cancelPay(){$("#isPayed").val("false"); return true;}&lt;/script&gt;&lt;/body&gt;&lt;/html&gt;</value>
  </data>
